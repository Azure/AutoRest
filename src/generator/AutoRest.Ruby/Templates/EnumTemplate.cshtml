@using AutoRest.Core
@inherits AutoRest.Core.Template<AutoRest.Core.Model.EnumType>
# encoding: utf-8
@Header("# ")
@EmptyLine
module @Settings.Namespace
  module Models
    #
    @WrapComment("# ", "Defines values for " + Model.Name)
    #
    module @(CodeNamer.Instance.PascalCase(Model.Name))
      @for (int i = 0; i < Model.Values.Count; i++)
      {
      @:@(CodeNamer.Instance.PascalCase(CodeNamer.Instance.RemoveInvalidCharacters(Model.Values[i].Name))) = "@(Model.Values[i].SerializedName)"
      }
    end
  end
end
