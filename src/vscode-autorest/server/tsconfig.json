{
	"compilerOptions": {
    "alwaysStrict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "sourceMap": true,
    "strictNullChecks": false,
    "stripInternal": true,
    "target": "es2016",
    "types": [
      "node"
    ],
    "lib": [
      "esnext"
    ],
    "experimentalDecorators": true,
    "newLine": "LF"


	},
  "include": [
    "src/server.ts"
  ],
	"exclude": [
		"node_modules/**",
    "../../autorest/**",
    "../../autorest-core/**"
	]
}