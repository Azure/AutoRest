    },
    "Microsoft.Rest.Generator.Ruby": {
      "target": "project"
    },
    "Microsoft.Rest.Generator.Java": {
      "target": "project"
    },
    "Microsoft.Rest.Generator.NodeJS": {
      "target": "project"
    },
    "Microsoft.Rest.Generator.Python": {
      "target": "project"
    },
    "Microsoft.Rest.Generator.CSharp": {
