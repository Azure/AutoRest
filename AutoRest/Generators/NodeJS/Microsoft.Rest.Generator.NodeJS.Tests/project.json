{
  "version": "1.0.0-*",
  "authors": [ "Microsoft" ],

  "packOptions": {
    "summary": "Autorest NodeJS Generator Tests ",
    "tags": [ "Microsoft AutoRest " ],
    "projectUrl": "https://github.com/Azure/AutoRest",
    "licenseUrl": "https://raw.githubusercontent.com/Microsoft/dotnet/master/LICENSE"
  },
  "testRunner": "xunit",
  "frameworks": {
    "net451": {
    }
  },

 "buildOptions": {
    "delaySign": true,
    "publicSign": false,
    "keyFile": "../../../../Tools/MSSharedLibKey.snk",

    "copyToOutput": {
      "include": ["Expected" ],
      "mappings" : {
        "AutoRest.json" : "../../../Microsoft.Rest.Generator.Cli/AutoRest.json",
        "Swagger/body-complex.json" : "../../../TestServer/swagger/body-complex.json",
        "Swagger/complex-model.json" : "../../../TestServer/swagger/complex-model.json"
      }
    }
  },

  "dependencies": {
    "xunit": "2.1.0",
    "dotnet-test-xunit": "1.0.0-rc2-build10025",
    "Newtonsoft.Json": "8.0.3",

    "Microsoft.Rest.Generator": {
      "target": "project"
    },
    "Microsoft.Rest.Generator.NodeJS": {
      "target": "project"
    },
    "Microsoft.Rest.Modeler.Swagger.Tests": {
      "target": "project"
    }
  }
}
