import "@cadl-lang/rest";
import "@azure-tools/cadl-azure-core";

using Cadl.Rest;
using Azure.Core;

namespace AutoRestPagingTestService;

enum OperationResult_status {
  "Succeeded",
  "Failed",
  "canceled",
  "Accepted",
  "Creating",
  "Created",
  "Updating",
  "Updated",
  "Deleting",
  "Deleted",
  "OK",
}

model ProductResultValue is Azure.Core.Page<Product>;

model Product {
  properties?: Product_properties;
}

model Product_properties {
  id?: int32;
  name?: string;
}

@pagedResult
model ProductResult {
  @items
  values?: Product[];

  @nextLink
  nextLink?: string;
}

@pagedResult
model OdataProductResult {
  @items
  values?: Product[];

  @nextLink
  "odata.nextLink"?: string;
}

@pagedResult
model ProductResultValueWithXMSClientName {
  @items
  @projectedName("json", "values")
  Indexes?: Product[];

  @nextLink
  nextLink?: string;
}

model OperationResult {
  @doc("The status of the request")
  status?: OperationResult_status;
}
